        return when (this) {
            ALPHABETICAL -> "Alphabetical"
            LAST_MODIFIED -> "Last Modified"
            PROJECT_THEN_ALPHA -> "Project then Alphabetical"
            MODULE_THEN_ALPHA -> "Module then Alphabetical"
        }
    }
}
